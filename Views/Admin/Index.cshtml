@model List<UserModelDatos>
@{
    ViewData["Title"] = "Home Page";
  
}
@Html.ActionLink("Agregar Administrador", "Añadir", "Admin", null, new { role="button", @class="btn btn-warning"})
<div class="text-center">
    <h1 class="display-4">Registro de Administradores </h1>
    <br>
    
    <br>
    <table class="table table-striped table-bordered">
        <thead>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Profesion</th>
            <th>Email</th>
            <th>DNI</th>
            <th>Fecha de naciomiento</th> 
            <th>Editar</th>
            <th>Eliminar</th> 
            <th>Turno</th>      
        </thead>
        <tbody>
             @foreach (var item in @Model){
                 @if ( item.role =="Administrador")
                 {
                     <tr>
                 <td>@item.nombre</td>
        <td>@item.apellido</td>
        <td>@item.role</td>
        <td>@item.email</td>
        <td>@item.dni</td>
        <td>@item.fechadenacimiento</td>
        <td>@Html.ActionLink("Editar", "Editar", "Admin", new  { userid = @item.userid }, new {target="_blank", @class="btn btn-primary my-2", role="button"})</td>
        <td> @Html.ActionLink("Eliminar", "Eliminar", "Admin", new  {  userid = @item.userid }, new{ role="button", @class="btn btn-danger"} )</td>

            </tr>
       
                 }
            
        
        
    }
        </tbody>
    </table>
</div>


 